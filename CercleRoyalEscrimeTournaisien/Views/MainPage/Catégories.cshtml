<div class="jumbotronCatégories">
    <div style="text-align:right;">
        <i class="fa fa-bars fa-beat" onclick="ShowStartPage();" style="color: red; font-size: 30px;"></i>
    </div>

    <p class="cssForTitreCategories">@Html.Raw(@CercleRoyalEscrimeTournaisien.Categories.Title)</p>
    <table class="cssForCategories" style="display: inline-grid;">

        @for (int iloop = 0; iloop < Model.RowsOfCol_1.Count(); iloop++)
        {
            <tr>
                <td style="width:240px;">@Model.RowsOfCol_1[iloop]</td>
                <td style="width:240px;">@Model.RowsOfCol_4[iloop]</td>
            </tr>
        }
    </table>
</div>